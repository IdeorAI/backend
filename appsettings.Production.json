{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Gemini": {
    "ApiKey": "${GEMINI_API_KEY}",
    "ProjectNumber": "855568455027",
    "Model": "gemini-2.5-flash",
    "Temperature": "0.9",
    "MaxOutputTokens": "2048"
  },
  "ConnectionStrings": {
    "SupabaseConnection": "${SUPABASE_CONNECTION_STRING}"
  },
  "Supabase": {
    "Url": "${SUPABASE_URL}",
    "AnonKey": "${SUPABASE_ANON_KEY}",
    "ServiceRoleKey": "${SUPABASE_SERVICE_ROLE_KEY}"
  },
  "Otlp": {
    "Endpoint": "${OTLP_ENDPOINT}"
  },
  "Jaeger": {
    "Host": "${JAEGER_HOST}",
    "Port": "${JAEGER_PORT}"
  },
  "Tempo": {
    "Endpoint": "${TEMPO_ENDPOINT}"
  }
}
